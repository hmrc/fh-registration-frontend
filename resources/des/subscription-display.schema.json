{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "84EZ - FHDDS - Subscription Display - Response Schema",
  "type": "object",
  "properties": {
    "subScriptionDisplay": {
      "$ref": "#/definitions/subscriptionDisplayType"
    }
  },
  "additionalProperties": false,
  "definitions": {
    "subscriptionDisplayType": {
      "type": "object",
      "properties": {
        "organizationType": {
          "type": "string",
          "enum": [
            "Sole Proprietor",
            "Limited Liability Partnership",
            "Partnership",
            "Corporate Body",
            "Unincorporated Body"
          ]
        },
        "FHbusinessDetail": {
          "$ref": "#/definitions/FHbusinessDetailType"
        },
        "additionalBusinessInformation": {
          "$ref": "#/definitions/additionalBusinessInformationType"
        },
        "businessDetail": {
          "type": "object",
          "properties": {
            "soleProprietor": {
              "$ref": "#/definitions/soleProprietorType"
            },
            "nonProprietor": {
              "$ref": "#/definitions/nonProprietorType"
            },
            "partnership": {
              "$ref": "#/definitions/partnerShipBusinessDetailsType"
            }
          },
          "additionalProperties": false
        },
        "businessAddressForFHDDS": {
          "$ref": "#/definitions/businessAddressForFHDDSType"
        },
        "contactDetail": {
          "$ref": "#/definitions/contactDetailsType"
        },
        "declaration": {
          "$ref": "#/definitions/declarationType"
        }
      },
      "required": [
        "organizationType",
        "FHbusinessDetail",
        "additionalBusinessInformation",
        "businessDetail",
        "businessAddressForFHDDS",
        "contactDetail",
        "declaration"
      ],
      "additionalProperties": false
    },
    "FHbusinessDetailType": {
      "type": "object",
      "properties": {
        "isNewFulfilmentBusiness": {
          "type": "boolean"
        },
        "proposedStartDate": {
          "$ref": "#/definitions/dateString"
        }
      },
      "required": [
        "isNewFulfilmentBusiness"
      ],
      "additionalProperties": false
    },
    "additionalBusinessInformationType": {
      "type": "object",
      "properties": {
        "partnerCorporateBody": {
          "type": "object",
          "properties": {
            "numberOfOtherOfficials": {
              "type": "string",
              "pattern": "^[0-9]{1,10}$"
            },
            "companyOfficials": {
              "type": "array",
              "minItems": 1,
              "items": {
                "oneOf": [
                  {
                    "type": "object",
                    "properties": {
                      "role": {
                        "type": "string",
                        "enum": [
                          "Director",
                          "Company Secretary",
                          "Director and Company Secretary",
                          "Member"
                        ]
                      },
                      "name": {
                        "$ref": "#/definitions/namesType"
                      },
                      "identification": {
                        "type": "object",
                        "properties": {
                          "passportNumber": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 20
                          },
                          "nationalIdNumber": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 20
                          },
                          "nino": {
                            "type": "string",
                            "pattern": "^((?!(BG|GB|KN|NK|NT|TN|ZZ)|(D|F|I|Q|U|V)[A-Z]|[A-Z](D|F|I|O|Q|U|V))[A-Z]{2})[0-9]{6}[A-D]?$"
                          }
                        },
                        "additionalProperties": false
                      }
                    },
                    "required": [
                      "role",
                      "name",
                      "identification"
                    ],
                    "additionalProperties": false
                  },
                  {
                    "type": "object",
                    "properties": {
                      "role": {
                        "type": "string",
                        "enum": [
                          "Director",
                          "Company Secretary",
                          "Director and Company Secretary",
                          "Member"
                        ]
                      },
                      "name": {
                        "type": "object",
                        "properties": {
                          "companyName": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 140
                          },
                          "tradingName": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 120
                          }
                        },
                        "additionalProperties": false
                      },
                      "identification": {
                        "type": "object",
                        "properties": {
                          "vatRegistrationNumber": {
                            "type": "string",
                            "pattern": "^[0-9]{9}$"
                          },
                          "uniqueTaxpayerReference": {
                            "type": "string",
                            "pattern": "^[0-9]{10}$"
                          },
                          "companyRegistrationNumber": {
                            "type": "string",
                            "pattern": "^[A-Z0-9]{8}$"
                          }
                        },
                        "additionalProperties": false
                      }
                    },
                    "required": [
                      "role",
                      "name",
                      "identification"
                    ],
                    "additionalProperties": false
                  }
                ]
              }
            }
          },
          "required": [
            "numberOfOtherOfficials"
          ],
          "additionalProperties": false
        },
        "allOtherInformation": {
          "type": "object",
          "properties": {
            "numberOfCustomers": {
              "type": "string",
              "enum": [
                "None",
                "1-10",
                "11-50",
                "51-100",
                "Over 100"
              ]
            },
            "doesEORIExist": {
              "type": "boolean"
            },
            "EORINumber": {
              "$ref": "#/definitions/EORINumberType"
            },
            "numberOfpremises": {
              "type": "string",
              "pattern": "^[0-9]{1,10}$"
            },
            "premises": {
              "type": "array",
              "minItems": 0,
              "items": {
                "properties": {
                  "address": {
                    "$ref": "#/definitions/addressType"
                  },
                  "thirdPartyPremises": {
                    "description": "This field comes with per address",
                    "type": "boolean"
                  }
                },
                "required": [
                  "thirdPartyPremises",
                  "address"
                ],
                "additionalProperties": false
              }
            }
          },
          "required": [
            "doesEORIExist",
            "numberOfCustomers",
            "numberOfpremises"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "allOtherInformation"
      ],
      "additionalProperties": false
    },
    "businessAddressForFHDDSType": {
      "type": "object",
      "properties": {
        "currentAddress": {
          "$ref": "#/definitions/addressType"
        },
        "commonDetails": {
          "$ref": "#/definitions/commonDetailsType"
        },
        "timeOperatedAtCurrentAddress": {
          "type": "string",
          "enum": [
            "Less than 3 years",
            "3-5 years",
            "5-10 years",
            "10 or more years"
          ]
        },
        "previousOperationalAddress": {
          "type": "object",
          "properties": {
            "anyPreviousOperatingAddress": {
              "type": "boolean"
            },
            "previousOperationalAddressDetail": {
              "type": "array",
              "minItems": 0,
              "items": {
                "type": "object",
                "properties": {
                  "previousAddress": {
                    "$ref": "#/definitions/addressType"
                  },
                  "previousAddressStartdate": {
                    "$ref": "#/definitions/dateString"
                  }
                },
                "required": [
                  "previousAddress"
                ],
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false,
          "required": [
            "anyPreviousOperatingAddress"
          ]
        }
      },
      "required": [
        "currentAddress",
        "commonDetails",
        "timeOperatedAtCurrentAddress"
      ],
      "additionalProperties": false
    },
    "contactDetailsType": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "minLength": 2,
          "maxLength": 30
        },
        "names": {
          "$ref": "#/definitions/namesType"
        },
        "usingSameContactAddress": {
          "type": "boolean"
        },
        "address": {
          "$ref": "#/definitions/addressType"
        },
        "commonDetails": {
          "$ref": "#/definitions/commonDetailsType"
        },
        "roleInOrganization": {
          "type": "object",
          "properties": {
            "beneficialShareHolder": {
              "type": "boolean"
            },
            "director": {
              "type": "boolean"
            },
            "partner": {
              "type": "boolean"
            },
            "internalAccountant": {
              "type": "boolean"
            },
            "soleProprietor": {
              "type": "boolean"
            },
            "nominatedOfficer": {
              "type": "boolean"
            },
            "designatedmember": {
              "type": "boolean"
            },
            "otherRoleType": {
              "type": "boolean"
            },
            "otherRoleDescription": {
              "type": "string",
              "pattern": "^[a-zA-Z &`\\-\\'^]{1,40}$"
            }
          },
          "required": [
            "beneficialShareHolder",
            "director",
            "partner",
            "internalAccountant",
            "soleProprietor",
            "nominatedOfficer",
            "designatedmember",
            "otherRoleType"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "names",
        "usingSameContactAddress",
        "commonDetails"
      ],
      "additionalProperties": false
    },
    "declarationType": {
      "type": "object",
      "properties": {
        "personName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 140
        },
        "personStatus": {
          "type": "string",
          "minLength": 1,
          "maxLength": 140
        },
        "email": {
          "type": "string",
          "maxLength": 132,
          "format": "email"
        },
        "isInformationAccurate": {
          "type": "boolean"
        }
      },
      "required": [
        "personName",
        "personStatus",
        "isInformationAccurate"
      ],
      "additionalProperties": false
    },
    "addressType": {
      "description": "Captures address",
      "type": "object",
      "properties": {
        "line1": {
          "$ref": "#/definitions/AddressLineType"
        },
        "line2": {
          "$ref": "#/definitions/AddressLineType"
        },
        "line3": {
          "$ref": "#/definitions/AddressLineType"
        },
        "line4": {
          "$ref": "#/definitions/AddressLineType"
        },
        "postalCode": {
          "description": "This is populated if the address supplied is a UK address.",
          "type": "string",
          "pattern": "^[A-Za-z]{1,2}[0-9][0-9A-Za-z]?\\s*?[0-9][A-Za-z]{2}$"
        },
        "countryCode": {
          "$ref": "#/definitions/countryCodes"
        }
      },
      "required": [
        "line1",
        "countryCode"
      ],
      "additionalProperties": false
    },
    "AddressLineType": {
      "type": "string",
      "minLength": 1,
      "maxLength": 35
    },
    "commonDetailsType": {
      "type": "object",
      "properties": {
        "telephone": {
          "type": "string",
          "minLength": 1,
          "maxLength": 24
        },
        "mobileNumber": {
          "type": "string",
          "minLength": 1,
          "maxLength": 24
        },
        "email": {
          "type": "string",
          "maxLength": 132,
          "format": "email"
        }
      },
      "additionalProperties": false
    },
    "namesType": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 35
        },
        "middleName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 35
        },
        "lastName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 35
        }
      },
      "required": [
        "firstName",
        "lastName"
      ],
      "additionalProperties": false
    },
    "soleProprietorType": {
      "type": "object",
      "properties": {
        "tradingName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 120
        },
        "identification": {
          "type": "object",
          "properties": {
            "nino": {
              "type": "string",
              "pattern": "^((?!(BG|GB|KN|NK|NT|TN|ZZ)|(D|F|I|Q|U|V)[A-Z]|[A-Z](D|F|I|O|Q|U|V))[A-Z]{2})[0-9]{6}[A-D]?$"
            },
            "vatRegistrationNumber": {
              "type": "string",
              "pattern": "^[0-9]{9}$"
            },
            "uniqueTaxpayerReference": {
              "type": "string",
              "pattern": "^[0-9]{10}$"
            }
          },
          "additionalProperties": false
        }
      },
      "required": [
        "identification"
      ],
      "additionalProperties": false
    },
    "nonProprietorType": {
      "type": "object",
      "properties": {
        "tradingName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 120
        },
        "identification": {
          "type": "object",
          "properties": {
            "vatRegistrationNumber": {
              "type": "string",
              "pattern": "^[0-9]{9}$"
            },
            "uniqueTaxpayerReference": {
              "type": "string",
              "pattern": "^[0-9]{10}$"
            }
          },
          "additionalProperties": false
        }
      },
      "required": [
        "identification"
      ],
      "additionalProperties": false
    },
    "partnerShipBusinessDetailsType": {
      "type": "object",
      "properties": {
        "numbersOfPartners": {
          "type": "string",
          "pattern": "^[0-9]{1,5}$"
        },
        "partnerDetails": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "properties": {
              "entityType": {
                "type": "string",
                "enum": [
                  "Sole Proprietor",
                  "Limited Liability Partnership",
                  "Partnership",
                  "Corporate Body",
                  "Unincorporated Body",
                  "Individual"
                ]
              },
              "partnerAddress": {
                "$ref": "#/definitions/addressType"
              },
              "partnerTypeDetail": {
                "oneOf": [
                  {
                    "$ref": "#/definitions/soleProprietorOrIndividualType"
                  },
                  {
                    "$ref": "#/definitions/limitedLiabilityPartnershipType"
                  },
                  {
                    "$ref": "#/definitions/partnershipOrUnIncorporatedBodyType"
                  }
                ]
              }
            },
            "required": [
              "entityType",
              "partnerAddress",
              "partnerTypeDetail"
            ],
            "additionalProperties": false
          }
        }
      },
      "required": [
        "numbersOfPartners",
        "partnerDetails"
      ],
      "additionalProperties": false
    },
    "soleProprietorOrIndividualType": {
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "nino": {
              "type": "string",
              "pattern": "^((?!(BG|GB|KN|NK|NT|TN|ZZ)|(D|F|I|Q|U|V)[A-Z]|[A-Z](D|F|I|O|Q|U|V))[A-Z]{2})[0-9]{6}[A-D]?$"
            },
            "name": {
              "$ref": "#/definitions/namesType"
            }
          },
          "required": [
            "name"
          ],
          "additionalProperties": false
        },
        {
          "type": "object",
          "properties": {
            "tradingName": {
              "type": "string",
              "minLength": 1,
              "maxLength": 120
            },
            "name": {
              "$ref": "#/definitions/namesType"
            },
            "nino": {
              "type": "string",
              "pattern": "^((?!(BG|GB|KN|NK|NT|TN|ZZ)|(D|F|I|Q|U|V)[A-Z]|[A-Z](D|F|I|O|Q|U|V))[A-Z]{2})[0-9]{6}[A-D]?$"
            },
            "identification": {
              "type": "object",
              "properties": {
                "vatRegistrationNumber": {
                  "type": "string",
                  "pattern": "^[0-9]{9}$"
                },
                "uniqueTaxpayerReference": {
                  "type": "string",
                  "pattern": "^[0-9]{10}$"
                }
              },
              "additionalProperties": false
            }
          },
          "required": [
            "name",
            "identification"
          ],
          "additionalProperties": false
        }
      ]
    },
    "limitedLiabilityPartnershipType": {
      "type": "object",
      "properties": {
        "names": {
          "type": "object",
          "properties": {
            "companyName": {
              "type": "string",
              "minLength": 1,
              "maxLength": 140
            },
            "tradingName": {
              "type": "string",
              "minLength": 1,
              "maxLength": 120
            }
          },
          "additionalProperties": false
        },
        "incorporationDetails": {
          "type": "object",
          "properties": {
            "companyRegistrationNumber": {
              "type": "string",
              "pattern": "^[A-Z0-9]{8}$"
            },
            "dateOfIncorporation": {
              "$ref": "#/definitions/dateString"
            }
          },
          "additionalProperties": false
        },
        "identification": {
          "type": "object",
          "properties": {
            "vatRegistrationNumber": {
              "type": "string",
              "pattern": "^[0-9]{9}$"
            },
            "uniqueTaxpayerReference": {
              "type": "string",
              "pattern": "^[0-9]{10}$"
            }
          },
          "additionalProperties": false
        }
      },
      "required": [
        "names",
        "incorporationDetails",
        "identification"
      ],
      "additionalProperties": false
    },
    "partnershipOrUnIncorporatedBodyType": {
      "type": "object",
      "properties": {
        "names": {
          "type": "object",
          "properties": {
            "companyName": {
              "type": "string",
              "minLength": 1,
              "maxLength": 140
            },
            "tradingName": {
              "type": "string",
              "minLength": 1,
              "maxLength": 120
            }
          },
          "additionalProperties": false
        },
        "identification": {
          "type": "object",
          "properties": {
            "vatRegistrationNumber": {
              "type": "string",
              "pattern": "^[0-9]{9}$"
            },
            "uniqueTaxpayerReference": {
              "type": "string",
              "pattern": "^[0-9]{10}$"
            }
          },
          "additionalProperties": false
        }
      },
      "required": [
        "names",
        "identification"
      ],
      "additionalProperties": false
    },
    "EORINumberType": {
      "type": "object",
      "properties": {
        "EORIVat": {
          "type": "string",
          "minLength": 1,
          "maxLength": 15
        },
        "EORINonVat": {
          "type": "string",
          "minLength": 1,
          "maxLength": 15
        },
        "goodsImportedOutEORI": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
